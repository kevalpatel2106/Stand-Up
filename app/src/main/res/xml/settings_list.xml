<android.support.v7.preference.PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <!--Sync settings-->
    <com.kevalpatel2106.standup.settings.widget.BasePreference
        android:icon="@drawable/ic_sync"
        android:key="@string/key_sync_settings_heading"
        android:selectable="true"
        android:title="Sync" />

    <!--DND settings-->
    <com.kevalpatel2106.standup.settings.widget.BasePreference
        android:icon="@drawable/ic_dnd_on"
        android:key="@string/key_dnd_settings_heading"
        android:selectable="true"
        android:title="Do not disturb" />

    <!--Notifications settings-->
    <com.kevalpatel2106.standup.settings.widget.BasePreference
        android:icon="@drawable/ic_notifications_bell"
        android:key="@string/key_notification_settings_heading"
        android:selectable="true"
        android:title="Notifications" />

    <!--Logout -->
    <com.kevalpatel2106.standup.settings.widget.BasePreference
        android:icon="@drawable/ic_logout"
        android:key="@string/key_logout_settings_heading"
        android:selectable="true"
        android:title="Logout" />
</android.support.v7.preference.PreferenceScreen>